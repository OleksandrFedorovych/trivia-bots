{
  "name": "trivia-bots",
  "version": "1.0.0",
  "description": "Scalable browser automation system for trivia games on Crowd.live",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "bot": "node src/bot.js",
    "bots": "node src/runMultipleBots.js",
    "bots:5": "node src/runMultipleBots.js 5",
    "bots:10": "node src/runMultipleBots.js 10",
    "bots:25": "node src/runMultipleBots.js 25",
    "scheduler": "node src/runScheduler.js",
    "scheduler:status": "node src/runScheduler.js status",
    "scheduler:nfl": "node src/runScheduler.js run nfl",
    "scheduler:hockey": "node src/runScheduler.js run hockey",
    "test": "node src/test.js",
    "dev": "node --watch src/index.js"
  },
  "keywords": [
    "trivia",
    "automation",
    "playwright",
    "crowd.live"
  ],
  "author": "Oleksandr Fedorovych",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.6.1",
    "googleapis": "^144.0.0",
    "playwright": "^1.57.0",
    "winston": "^3.18.3"
  }
}